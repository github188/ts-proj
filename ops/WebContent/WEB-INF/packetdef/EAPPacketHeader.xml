<?xml version="1.0" encoding="UTF-8" ?>
<eapPacketHeader>
	<element>
		<name>CODE</name>
		<description>代号~EAP</description>
		<length>4</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- 放入配置文件中 -->
	</element>
	<element>
		<name>APPCODE</name>
		<description>应用代号</description>
		<length>2</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- 配置文件中 -->
	</element>
	<element>
		<name>CHANNELCODE</name>
		<description>渠道代号</description>
		<length>2</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- 配置文件 -->
	</element>
	<element>
		<name>TRCODE</name>
		<description>交易代号</description>
		<length>4</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- API中指定 -->
	</element>
	<element>
		<name>UNITNO</name>
		<description>机构号</description>
		<length>4</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- 配置文件 -->
	</element>
	<element>
		<name>DEVICENO</name>
		<description>设备号</description>
		<length>8</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- 配置文件 -->
	</element>
	<element>
		<name>TELLID</name>
		<description>柜员号</description>
		<length>6</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- 配置文件 -->
	</element>
	<element>
		<name>OTP_CARD_TYPE</name>
		<description>卡类型</description>
		<length>3</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- API中填写,若方法参数中有此值,则填 -->
	</element>
	<element>
		<name>TRDATE</name>
		<description>发送日期</description>
		<length>8</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- API中填写,与填写配置文件中的内容一起 -->
	</element>
	<element>
		<name>TRTIME</name>
		<description>发送时间</description>
		<length>6</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- API中填写,与填写配置文件中的内容一起 -->
	</element>
	<element>
		<name>SEQNO</name>
		<description>发送序号</description>
		<length>6</length>
		<fillCharacter>'0'</fillCharacter>
		<fillMode>left</fillMode>
		<!-- API中填写,与填写配置文件中的内容一起 -->
	</element>
	<element>
		<name>TOTALREC</name>
		<description>记录数</description>
		<length>5</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>left</fillMode>
		<!-- 不填 -->
	</element>
	<element>
		<name>BEGINREC</name>
		<description>起始笔数</description>
		<length>5</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>left</fillMode>
		<!-- 可填可不填 -->
	</element>
	<element>
		<name>RECCOUNT</name>
		<description>本次记录数</description>
		<length>3</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>left</fillMode>
		<!-- 填写 -->
	</element>
	<element>
		<name>RETCODE</name>
		<description>返回码</description>
		<length>6</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>right</fillMode>
		<!-- 不填 -->
	</element>
	<element>
		<name>PACKETLENGTH</name>
		<description>报文长度</description>
		<length>4</length>
		<fillCharacter>' '</fillCharacter>
		<fillMode>left</fillMode>
		<!-- 不填 -->
	</element>
</eapPacketHeader>